<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iot.quickhpu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.GradeActivity" />
        <activity android:name=".activity.GradeAllActivity" />
        <activity android:name=".activity.EmptyClassroomActivity" />
        <activity android:name=".activity.StudentManagerActivity" />
        <activity android:name=".activity.StudentDetailActivity" />
        <activity android:name=".activity.InfoDetailActivity" />
        <activity android:name=".activity.PublishTopicActivity" />
        <activity android:name=".activity.CreateSignActivity" />
        <activity android:name=".activity.ClassManagerActivity" />
        <activity android:name=".activity.CreateClassActivity" />
        <activity android:name=".activity.TopicDetailActivity" />
        <activity android:name=".activity.CommentListActivity" />
        <activity android:name=".activity.PublishCommentActivity"></activity>
    </application>

</manifest>